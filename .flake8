[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501, E402, E712, F541, F821, F841, F811, F402, E266
exclude =
    .git,
    __pycache__,
    migrations,
    .venv,
    venv,
    build,
    dist,
    alembic/versions
per-file-ignores =
    __init__.py:F401,F403
    */migrations/*:E501
    */alembic/versions/*:E501,F401
    alembic/env.py:F401,F403
