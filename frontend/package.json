{
  "name": "shrutik_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.13.1",
    "clsx": "^2.1.1",
    "i18next": "^23.0.0",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^7.9.5",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.9"
  },
  "scripts": {
    "dev": "vite",
    "dev:monitor": "node scripts/dev-performance.js",
    "start": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true npm run build && node scripts/analyze-bundle.js",
    "build:performance": "node scripts/build-performance.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:build-performance": "vitest --run src/__tests__/build-performance.test.ts --config src/__tests__/vitest.node.config.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "lint": "node scripts/check-unused-imports.js",
    "lint:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "deps:check": "node scripts/analyze-bundle.js",
    "perf:dev": "node scripts/dev-performance.js",
    "perf:build": "node scripts/build-performance.js",
    "cache:clear": "node .vitecache.config.js clear",
    "cache:stats": "node .vitecache.config.js stats",
    "cache:optimize": "node .vitecache.config.js optimize",
    "sourcemap:validate": "node sourcemap.config.js validate build",
    "sourcemap:analyze": "node sourcemap.config.js analyze build",
    "sourcemap:config": "node sourcemap.config.js config production",
    "test:optimizations": "node test-optimizations.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "fast-check": "^4.4.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.15"
  }
}
